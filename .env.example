# MQTT Configuration (optional)
# If not set, will use default broker URL
VITE_MQTT_BROKER_URL=ws://localhost:8000/mqtt

# MQTT Authentication (required for non-localhost deployments)
# Public credentials used for regular users
VITE_MQTT_PUBLIC_USERNAME=
VITE_MQTT_PUBLIC_PASSWORD=
# Admin username (password is entered by user at runtime)
VITE_MQTT_ADMIN_USERNAME=

# Voice Server Configuration (optional)
# Required for voice functionality
VITE_VOICE_SERVER_ENDPOINT=
VITE_VOICE_SERVER_BEARER_TOKEN=

# PostHog Analytics Configuration (optional)
# Leave these empty to run without analytics
# PostHog will be automatically disabled when VITE_PUBLIC_POSTHOG_KEY is not set
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=https://eu.i.posthog.com

# Build Information (automatically set during build)
# These are populated by the build process
VITE_BUILDTIME=
VITE_VERSION=
VITE_REVISION=